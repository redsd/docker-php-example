APP_ENV=local

#Opcache
PHP_OPCACHE_VALIDATE_TIMESTAMPS=1
PHP_OPCACHE_MEMORY_CONSUMPTION=192
PHP_OPCACHE_MAX_ACCELERATED_FILES=15000
PHP_OPCACHE_MAX_WASTED_PERCENTAGE=10

PHP_XDEBUG_DEFAULT_ENABLE=1
#on windows Ethernet adapter vEthernet (Primary Virtual Switch): IPv4 address
PHP_XDEBUG_REMOTE_HOST=192.168.52.129
#@office
#PHP_XDEBUG_REMOTE_HOST=192.168.24.174
PHP_XDEBUG_REMOTE_AUTOSTART=1
PHP_XDEBUG_REMOTE_PORT=9001
PHP_XDEBUG_REMOTE_ENABLE=1
PHP_XDEBUG_IDEKEY=DOCKER_XDEBUG
PHP_XDEBUG_PROFILER_ENABLE=0
PHP_XDEBUG_PROFILER_OUTPUT_DIR=/var/www/html/storage/logs